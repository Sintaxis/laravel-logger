{
    "name": "crudlog/laravel-logger",
    "description": "The official CrudLog client package for implicitly logging activities in Laravel applications.",
    "keywords": [
        "crudlog",
        "laravel",
        "logging",
        "auditing",
        "activity-log"
    ],
    "homepage": "https://github.com/Sintaxis/laravel-logger",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Crudlog\\Logger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Panayiotis Pateras",
            "email": "pateras80@gmail.com",
            "homepage": "https://crudlog.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "illuminate/contracts": "^11.0",
        "illuminate/database": "^11.0",
        "illuminate/events": "^11.0",
        "illuminate/http": "^11.0",
        "illuminate/support": "^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CrudLog\\Logger\\Providers\\LoggableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
